{
    "manifest_version": 3,
    "name": "Custom Style Changer",
    "version": "1.0",
    "description": "Modify CSS and JavaScript of a specific website",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["https://*/*"],
        "js": ["content.js"],
        "css": ["newstyle.css"]
      }
    ]
  }
  